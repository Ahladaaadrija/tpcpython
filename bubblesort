def bubble(arr):
    n = len(arr)
    for i in range(n-1):
        swapped=False
        for j in range(n-1-i):
            if arr[j] > arr[j+1]:
                arr[j],arr[j+1] = arr[j+1],arr[j]
                swapped = True
        if swapped==False:
            break
    return arr
arr=[3,1,9,2,6]
print(bubble(arr))
